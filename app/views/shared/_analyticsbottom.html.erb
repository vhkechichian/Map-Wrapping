  <script type="text/javascript">
    try {
      var pageTracker = _gat._getTracker("<%=APP_CONFIG['google_analytics_code'] %>");
      pageTracker._setCookiePath("<%=APP_CONFIG['google_analytics_cookie_path'] %>");
      <%- if @layer -%>
      pageTracker._trackPageview("layers/<%=@current_tab%>/<%=@layer.id%>");
        <%- else -%>
      pageTracker._trackPageview("maps/<%=@current_tab%>/<%=@map.id%>");
        <%- end -%>
    } catch(err) {}</script>

