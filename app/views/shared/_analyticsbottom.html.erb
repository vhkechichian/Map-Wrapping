<% unless APP_CONFIG['google_analytics_code'].blank? %>
<script type="text/javascript">
  try {
    <%- if @layer -%>
    ga('send', 'pageview', "layers/<%=@current_tab%>/<%=@layer.id%>");
      <%- else -%>
    ga('send', 'pageview', "maps/<%=@current_tab%>/<%=@map.id%>");
      <%- end -%>
  } catch(err) {}
</script>
<% end %>

